@using TicariOtomasyon.Models.Siniflar
@model List<KargoTakip>
@{
    ViewBag.Title = "KargoTakip";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>KargoTakip</h2>


<table class="table table-hover">
    <tr>

        <th>Takip Kodu</th>
        <th>Açıklama</th>
        <th>Tarih</th>
       
    </tr>
    @foreach (var kt in Model)
    {
        <tr>
            <td>@kt.TakipKodu</td>
            <td>@kt.Aciklama</td>
            <td>@kt.TarihZaman</td>

        </tr>
    }
</table>